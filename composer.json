{
  "name": "attia/support",
  "description": "Laravel Helpers.",
  "license": "MIT",
  "keywords": [
    "attia",
    "support",
    "helpers",
    "laravel-helpers",
    "laravel"
  ],
  "authors": [
    {
      "name": "Attia Ahmed",
      "email": "attia.a.ahmed@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "laravel/framework": "^9.0|^10.0|^11.0",
    "ext-json": "*"
  },
  "require-dev": {
    "mockery/mockery": "^1.4",
    "orchestra/testbench": "^6.23|^7.0",
    "phpunit/phpunit": "^9.5.10",
    "laravel/pint": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Attia\\Support\\": "src/"
    },
    "files": [
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Attia\\Support\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Attia\\Support\\SupportBaseServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}